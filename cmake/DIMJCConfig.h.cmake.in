#ifndef DIMJCConfig_H
#define DIMJCConfig_H

// version macros
#define DIMJC_MAJOR_VERSION @DIMJC_VERSION_MAJOR@
#define DIMJC_MINOR_VERSION @DIMJC_VERSION_MINOR@
#define DIMJC_PATCH_LEVEL @DIMJC_VERSION_PATCH@
#define DIMJC_VERSION_STR "@DIMJC_VERSION_MAJOR@.@DIMJC_VERSION_MINOR@.@DIMJC_VERSION_PATCH@"
#define DIMJC_VERSION_GE( MAJV , MINV , PLEV )  \
     ( (DIMJC_MAJOR_VERSION > MAJV) || ( (DIMJC_MAJOR_VERSION == MAJV) && (DIMJC_MINOR_VERSION > MINV) ) \
  || ( (DIMJC_MAJOR_VERSION == MAJV) && (DIMJC_MINOR_VERSION == MINV) && (DIMJC_PATCH_LEVEL >= PLEV) ) )

// project source dir
// useful for icons
#define DIMJC_DIR "@PROJECT_SOURCE_DIR@"


#endif // DIMJCConfig_H
